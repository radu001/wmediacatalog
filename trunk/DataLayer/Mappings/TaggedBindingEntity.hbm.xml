<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="DataLayer" namespace="DataLayer.Entities">
  <class name="TaggedBindingEntity" table="tagged_objects" lazy="false">
    <composite-id>
      <key-property name="TagID" column="tag_id"/>
      <key-property name="EntityID" column="entity_id"/>
    </composite-id>
    <property name="EntityType" column="entity_type"/>
    <property name="TagName" column="tag_name"/>
    <property name="EntityName" column="entity_name"/>
  </class>
</hibernate-mapping>